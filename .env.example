# Target website URL to proxy
TARGET_URL=https://example.com

# Port for local development (not used in Vercel)
PORT=3000

# Your proxy domain (localhost for local, your-domain.vercel.app for Vercel)
PROXY_HOST=localhost:3000

# Enable/disable caching
ENABLE_CACHE=false

# Cache TTL in seconds
CACHE_TTL=3600

# Enable debug logging
DEBUG=false